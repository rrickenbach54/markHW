<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">
    <entity class="application.User">
        <table name="user" schema="mark_homework"/>
        <attributes>
            <id name="userId">
                <column name="user_id"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="firstName">
                <column name="first_name"/>
            </basic>
            <basic name="lastName">
                <column name="last_name"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="createTime">
                <column name="create_time"/>
            </basic>
            <basic name="lastLogin">
                <column name="last_login"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
